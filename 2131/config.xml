		<allowedDomain>localhost</allowedDomain>
		<allowedDomain>nick.com</allowedDomain>
		<allowedDomain>mtvi.com</allowedDomain>
		<allowedDomain>mtvnimages.com</allowedDomain>
	</allowedDomains>
