			<trackingPlayAgainFunction>trackKidsGamePlay</trackingPlayAgainFunction>
			<trackingPlayGameName>super-brawl-3</trackingPlayGameName> <!-- Needs custom data -->
			<trackingPrintFunction>printTracker</trackingPrintFunction>
			<trackingPrintSite>SITE_HERE</trackingPrintSite> <!-- Needs custom data -->
			<trackingPrintUrlAlias>super-brawl-3</trackingPrintUrlAlias> <!-- Needs custom data -->
